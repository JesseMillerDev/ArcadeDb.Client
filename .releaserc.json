{
  "branches": ["main"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    ["@semantic-release/exec", {
      "prepareCmd": "sed -i 's/<Version>.*<\\/Version>/<Version>${nextRelease.version}<\\/Version>/' src/ArcadeDB.Client/ArcadeDb.Client.csproj src/ArcadeDb.Client.Extras/ArcadeDb.Client.Extras.csproj"
    }],
    ["@semantic-release/git", {
      "assets": ["CHANGELOG.md", "src/ArcadeDB.Client/ArcadeDb.Client.csproj", "src/ArcadeDb.Client.Extras/ArcadeDb.Client.Extras.csproj"],
      "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
    }],
    "@semantic-release/github"
  ]
}
